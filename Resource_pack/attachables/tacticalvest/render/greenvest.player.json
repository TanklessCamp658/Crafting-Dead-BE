{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": { 
			"identifier": "cd:greenvest.player",
			"item": {
				"cd:greenvest": "query.owner_identifier == 'minecraft:player'"
			},
			"materials": {
				"default": "armor",
				"enchanted": "armor_enchanted"
			},
			"textures": {
				"default": "textures/models/vests/greenvest",
				"enchanted": "textures/misc/enchanted_item_glint"
			},
			"geometry": {
				"default": "geometry.tacticalvest"
			},
			"scripts": {
				"parent_setup": "variable.chest_layer_visible = 0.0;"
			},
			"render_controllers": [
				"controller.render.armor"
			]
		}
	}
}

